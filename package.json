{
  "name": "housing-repairs-online-frontend",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV='development' node server.js",
    "build": "next build",
    "start": "next start",
    "build:azure": "next build && next export",
    "lint": "next lint",
    "test": "jest tests/unit",
    "lint:fix": "eslint --fix */**/*.js tests/**/*.js",
    "cy:open": "cypress open --env RELEASE_VERSION=full --config baseUrl=http://localhost:3000",
    "cy:open:mvp": "cypress open --env RELEASE_VERSION=mvp --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --env RELEASE_VERSION=full --config baseUrl=http://localhost:3000",
    "cy:run:mvp": "cypress run --env RELEASE_VERSION=mvp --config baseUrl=http://localhost:3000 --spec 'tests/cypress/integration/reportRepair/*Mvp.spec.js'",
    "test:integration": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:integration:mvp": "env RELEASE_VERSION=mvp start-server-and-test dev http://localhost:3000 cy:open:mvp",
    "test:headless:integration": "start-server-and-test dev http://localhost:3000 cy:run",
    "test:headless:integration:mvp": "env RELEASE_VERSION=mvp start-server-and-test dev http://localhost:3000 cy:run:mvp"
  },
  "dependencies": {
    "@sentry/nextjs": "^6.17.4",
    "@zeit/next-sass": "^1.0.1",
    "azure-functions-core-tools": "3",
    "dotenv": "^10.0.0",
    "govuk-frontend": "^4.3.1",
    "image-to-base64": "^2.2.0",
    "libphonenumber-js": "^1.9.44",
    "moment": "^2.29.1",
    "next": "12.0.2",
    "node-fetch": "2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.15.0",
    "babel-eslint": "^10.1.0",
    "cypress": "9.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-react-selector": "^2.3.11",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-plugin-editorconfig": "^3.0.2",
    "eslint-plugin-react": "^7.25.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "jest": "^27.3.1",
    "node-sass": "^6.0.1",
    "start-server-and-test": "^1.14.0"
  }
}
